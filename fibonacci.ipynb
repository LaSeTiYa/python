    "#fibonacci\n",
    "count = int(input(\"輸入要加幾次:\"))\n",
    "f1 = 1\n",
    "f2 = 1\n",
    "tmp = 0\n",
    "for temp in range(1,count + 1):\n",
    "    if temp == 1:\n",
    "        print (f1)\n",
    "    elif temp == 2:\n",
    "        print (f2)\n",
    "    else:\n",
    "        tmp = f1 + f2\n",
    "        print (tmp)\n",
    "        f1 = f2\n",
    "        f2 = tmp"
   ]
  }
